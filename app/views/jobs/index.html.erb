
<h1>Jobs</h1>

<table>
  <thead>
    <tr>
      <th>Role</th>
      <th>Description</th>
      <th>Company</th>
      <th>Apply</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @jobs.each do |job| %>
      <tr>
        <td><%= job.role %></td>
        <td><%= job.description %></td>
        <td><%= job.company %></td>
        <td><%= job.apply %></td>
        <td><button type="button" class="btn btn-primary"><%= link_to 'Apply', new_apply_path %></button></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>


