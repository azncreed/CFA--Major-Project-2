<!DOCTYPE html>
<html>
  <head>
    <title>Project</title>
    <%= csrf_meta_tags %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
  	<%= render 'partials/nav' %>

    <div class="container">
      <% if notice %>
        <div class="alert alert-success">
        <%= notice %>
        </div>
      <% end %>

      <% if alert %>
        <div class="alert alert-warning">
        <%= alert %>
        </div>
      <% end %>

      
    <%= yield %>
  </body>
</html>
